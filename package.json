{
  "name": "helper-collection",
  "description": "Collection helper for assemble (or any other https://github.com/jonschlinkert/templates application)",
  "version": "0.2.1",
  "homepage": "https://github.com/helpers/helper-collection",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "helpers/helper-collection",
  "bugs": {
    "url": "https://github.com/helpers/helper-collection/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "assemble",
    "collection",
    "compile",
    "each",
    "helper",
    "helpers",
    "iterate",
    "pages",
    "posts",
    "render",
    "template",
    "templates"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "handlebars-helper-each",
        "handlebars-helpers",
        "template-helpers"
      ]
    },
    "reflinks": [
      "generate",
      "generate-helper",
      "helpers"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "devDependencies": {
    "assemble": "^0.22.0",
    "gulp-format-md": "^0.1.11",
    "handlebars-helper-each": "^0.1.2",
    "handlebars-helpers": "^0.8.0",
    "mocha": "^3.2.0"
  }
}
